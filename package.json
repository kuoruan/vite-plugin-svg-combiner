{
  "name": "vite-plugin-svg-combiner",
  "version": "0.5.1",
  "description": "Another svg sprites plugin for Vite",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/es/runtime.js",
      "require": "./dist/cjs/runtime.cjs",
      "types": "./dist/types/runtime.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "directory": "dist",
    "main": "./cjs/index.cjs",
    "module": "./es/index.js",
    "types": "./types/index.d.ts",
    "exports": {
      ".": {
        "import": "./es/index.js",
        "require": "./cjs/index.cjs",
        "types": "./types/index.d.ts"
      },
      "./runtime": {
        "import": "./es/runtime.js",
        "require": "./cjs/runtime.cjs",
        "types": "./types/runtime.d.ts"
      }
    }
  },
  "scripts": {
    "lint": "eslint --ext .ts,.js,.cjs,.mjs .",
    "build": "rimraf dist && rollup -c",
    "copy": "copyfiles package.json README.md LICENSE dist",
    "test": "vitest --run",
    "bundle": "pnpm build && pnpm copy"
  },
  "keywords": [
    "vite",
    "svg",
    "sprite",
    "svg-sprite",
    "svg-sprites",
    "combiner",
    "svg-combiner",
    "plugin"
  ],
  "author": "Xingwang Liao<kuoruan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuoruan/vite-plugin-svg-combiner.git"
  },
  "homepage": "https://github.com/kuoruan/vite-plugin-svg-combiner",
  "bugs": "https://github.com/kuoruan/vite-plugin-svg-combiner/issues",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "deepmerge": "^4.3.1",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-node-externals": "^7.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "rollup": ">=2.0.0",
    "vite": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "rollup": {
      "optional": true
    }
  }
}
